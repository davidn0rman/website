    "gatsby-transformer-sharp": "^2.5.12",
    "node-sass": "^7.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-jest": "^26.3.0",
    "babel-preset-gatsby": "^0.5.5",
    "classnames": "^2.2.6",
    "eslint": "^7.7.0",
