    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.3.0",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint --ignore-path .gitignore . --ext .ts,.tsx,.js,.jsx && npm run lint-sass",
    "lint:fix": "npm run lint --fix",
    "lint-legacy": "eslint --ext .tsx .",
    "lint-sass": "npx stylelint src/**/*.scss",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "type-check": "tsc --pretty",
    "update-deps-naughty": "npm update --save/--save-dev -f"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidn0rman/website.git"
  },
  "bugs": {
    "url": "https://github.com/davidn0rman/website/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint",
      "...": "..."
    }
  }
}
